#include<iostream>
#include <conio.h>

using namespace std;

int main()
{
    int n;
    cout<<"Enter the number of terms:\n";
    cin>>n;
    int fib(int a);
    for(int i=0;i<n;i++)
    {
        cout<<fib(i)<<", ";
    }
    return 0;
}
int fib(int a)
{
    if(a==0)
        return 0;
    if(a==1)
        return 1;
    else
    return (fib(a-1)+ fib(a-2));
}
